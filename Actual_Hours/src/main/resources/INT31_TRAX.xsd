                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="OPS_NO" minOccurs="1">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="10" />
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ACTUAL_WORK" minOccurs="1">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
